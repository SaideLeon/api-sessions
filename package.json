{
  "name": "api-sessions",
  "version": "1.0.0",
  "description": "API for managing user sessions, messages, vendors and sellers",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "npm run db:push && npm run prisma:generate",
    "db:push": "prisma db push",
    "db:reset": "prisma db push --force-reset",
    "db:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:format": "prisma format",
    "prisma:validate": "prisma validate",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf node_modules",
    "clean:full": "rm -rf node_modules && rm -rf dist && rm -rf coverage",
    "reinstall": "npm run clean && npm install"
  },
  "dependencies": {
    "@langchain/core": "^0.3.17",
    "@langchain/google-genai": "^0.1.3",
    "@langchain/groq": "^0.1.2",
    "@prisma/client": "^5.10.2",
    "archiver": "^6.0.1",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.1.5",
    "extract-zip": "^2.0.1",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^11.2.0",
    "groq-sdk": "^0.7.0",
    "helmet": "^7.1.0",
    "https-proxy-agent": "^7.0.4",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^2.1.35",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "node-webpmux": "^3.2.0",
    "puppeteer": "^22.8.2",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "sharp": "^0.33.2",
    "socket.io": "^4.7.4",
    "temp": "^0.9.4",
    "tmp": "^0.2.1",
    "unzipper": "^0.10.14",
    "uuid": "^9.0.1",
    "whatsapp-web.js": "^1.23.0",
    "winston": "^3.11.0",
    "ws": "^8.16.0",
    "yauzl": "^2.10.0",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@types/temp": "^0.9.4",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prisma": "^5.10.2",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=20.0.0 <21.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover"
    ]
  }
}